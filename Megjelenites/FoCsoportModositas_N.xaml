<UserControl x:Class="Sajat.Megjelenites.FoCsoportModositas_N"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Sajat.Megjelenites"
             xmlns:sajatwpf="clr-namespace:Sajat.WPF;assembly=Komponensek"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <sajatwpf:EgysegnyiValtozas 
        Rogzit="Rogziteskor" 
        Elvet="Elveteskor" 
        VanValtozas="{Binding VanValtozas, Mode=OneWay}" 
        RogzitesEredmeny="{Binding RogzitesEredmeny, Mode=OneWay}" 
    >
        <StackPanel >
            <sajatwpf:StatikusSzoveg Cimke="ID" Szoveg="{Binding FoCsoport.Id}"/>
            <sajatwpf:EllenorzottSzoveg Cimke="Név" Szoveg="{Binding FoCsoport.Nev}"/>
        </StackPanel>
    </sajatwpf:EgysegnyiValtozas>
</UserControl>
