<UserControl x:Class="Sajat.Partner.Valasztas_N"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Sajat.Partner"
             xmlns:sajatwpf="clr-namespace:Sajat.WPF;assembly=M_Komponensek_WPF"             
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.DataContext>
        <local:Valasztas_NM/>
    </UserControl.DataContext>
    <sajatwpf:Lekerdezes Lekerdez="Lekerdezeskor" Vissza="Visszakor">
        <sajatwpf:KetgombosLista TetelForras="{Binding Lista, Mode=OneWay}" Kivalaszt="Kivalasztaskor" Modosit="Modositaskor">
            <GridViewColumn DisplayMemberBinding="{Binding Id, StringFormat=\{0:D8\}}" Header="Id" Width="72" />
            <GridViewColumn DisplayMemberBinding="{Binding MJ}" Header="MJ" Width="50" />
            <GridViewColumn DisplayMemberBinding="{Binding Nev}" Header="Név" Width="200" />
        </sajatwpf:KetgombosLista>
    </sajatwpf:Lekerdezes>
</UserControl>
